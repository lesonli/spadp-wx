/* shopdetail.wxss */
.shop-detail-page{
  height: 100%;
  background: #eee;
}
.shop-detail-page .shop-img{
  width: 100%;
  height: 200px;
  vertical-align:middle;
}
.shop-detail-page .shop-info{
  min-height: 80px;
  background: #5677fc;
  padding: 10px;
  color: #FFF;
}
.shop-detail-page .shop-info .name{
  font-weight: 500;
  margin-bottom: 5px;
}
.shop-detail-page .shop-info .renjun{
  font-size:14px;
}
.shop-detail-page .shop-info .desc{
  font-size: 14px;
  margin-top: 5px;
}
.shop-detail-page .shop-desc{
  background: #FFF;
  min-height: 150px;
  margin-top: 5px;
  padding-left: 10px;
  padding-top: 10px;
}
.shop-detail-page .shop-desc .title{
  color: #353535;
  font-weight: 500;
}
.shop-detail-page .shop-desc .cell:first-child{
  border-top: 1rpx solid #eee;
  margin-top: 10px;
}
.shop-detail-page .shop-desc .cell{
  border-bottom: 1rpx solid #eee;
  height: 26px;
  line-height: 26px;
  padding:5px;
  font-size: 14px;
  color:#888;
  position: relative
}
.shop-detail-page .shop-desc .cell .icon{
  width:18px;
  height:18px;
  position:absolute;
  top:7px;
}

.shop-detail-page .shop-desc .cell .icon-right{
  width:18px;
  height:18px;
  position:absolute;
  top:10px;
  right:15px;
}
.shop-detail-page .shop-desc .cell .text{
  padding-left: 25px;
}
.block{
  display: block;
}
.margin-left-10{
  margin-left:10px;
}
.star{
    display: inline-block;
    width: 66px;
    height: 12px;
    vertical-align: middle;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 86px auto;
}
.star.star-50{
   background-position: 0 -15px;
}
.star.star-45{
   background-position: 0 -32px;
}
.star.star-40{
   background-position: 0 -49px;
}
.star.star-35{
   background-position: 0 -66px;
}
.star.star-30{
   background-position: 0 -83px;
}
.star.star-20{
   background-position: 0 -100px;
}
.star.star-10{
   background-position: 0 -117px;
}
.star.star-0{
   background-position: 0 -134px;
}